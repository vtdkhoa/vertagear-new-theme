$mobile-breakpoint: 768px;
$fhd-breakpoint: 1680px;
$gray-color: #c0c0c0;
$light-bg-color: #ffffff;
$dark-bg-color: #fbf9fa;
@mixin transition-box-shadow($box-shadow) {
  -webkit-box-shadow: $box-shadow;
  -moz-box-shadow: $box-shadow;
  box-shadow: $box-shadow;
}
@mixin border-box-shadow(){
  -webkit-box-shadow: 0px 0px 15px 0px rgba(133,133,133,0.2);
  -moz-box-shadow: 0px 0px 15px 0px rgba(133,133,133,0.2);
  box-shadow: 0px 0px 15px 0px rgba(133,133,133,0.2);
  background-color: #fff;
}
@mixin slick-components($top-spacing, $side-spacing, $arrow-color){
  .left-arrow, .right-arrow{
    position: absolute;
    top: $top-spacing;
    font-size: 60px;
    color: $arrow-color;
    z-index: 100;
    cursor: pointer;
  }
  .left-arrow{
    left: $side-spacing;
  }
  .right-arrow{
    right: $side-spacing;
  }
  ul.slick-dots{
    li{
      margin: 0;
      button{
        //border: 1px solid #000;
        //border-radius: 10px;
        &::before{
          text-align:center;
          //color: transparent;
          font-size:40px;
        }
      }
      &.slick-active{
        button::before{
          //color: #000; 
        }
      }
    }
  }
}
$desktop-margin-size: 8.6vw;
@mixin desktop-margins(){
  margin: 0 $desktop-margin-size;
}

$foamChartTextGray: #b2b2b2;
$foamChartWhite: #7a7f7f;
$foamChartOrange: #ef8f37;
@keyframes uphr-bar-glow {
  0% {
    fill: $foamChartWhite;
  }
  100% {
    fill: $foamChartOrange;
  }
}
@keyframes uphr-text-glow {
  0% {
    fill: $foamChartTextGray;
  }
  100% {
    fill: $foamChartOrange;
  }
}
