.tooltip_top,  .tooltip_bottom {
	display: block;	background-color: #fff;	text-align: center
}
.tooltip_top {
	height: 253px
}
.tooltip_top img {
	height: 100%;	width: 100%
}
.tooltip_top span {
	position: absolute;	color: #09AEED;	right: -10px;	padding: 0px 20px;	cursor: pointer;	font-size: 2em; z-index: 9999;
}
.tooltip_bottom {
	height: auto;	padding: 8px 20px;	background-color: #232323;	color: #fff;	text-align: left
}
.tooltip_content h2 {
	font-size: 1em;	letter-spacing: .05em;	margin: 0
}
.tooltip_content p {
	font-size: .9em
}
.tooltip_left,  .tooltip_right {
	width: 291px;	height: 315px;	float: left;	background-color: #fff;	text-align: left
}
#features_section .product-tooltips .popover {
	z-index:1060;border: 0;	background-color: transparent;	-webkit-box-shadow: 0 0 0 0 rgba(0, 0, 0, 0);	-moz-box-shadow: 0 0 0 0 rgba(0, 0, 0, 0);	box-shadow: 0 0 0 0 rgba(0, 0, 0, 0);	text-align: center;	max-width: 600px
}

.tooltip_top img{object-fit: contain;}



