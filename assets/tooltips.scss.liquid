#features_section {
    padding: 0 0 0 0;
}
#features_section h1 {
    color: #000;
}
#features_section .product-tooltips {
	width: 100%;	height: 100%;	text-align: center
}
#features_section .product-tooltips .tooltips-wrapper {
	position: relative;	display: inline-block;	top: 50%
}
#features_section .product-tooltips .tooltips-wrapper .popover.bottom,  #features_section .product-tooltips .tooltips-wrapper .popover.top {
	margin-top: 0
}
#features_section .product-tooltips .tooltips-wrapper .popover.right,  #features_section .product-tooltips .tooltips-wrapper .popover.left {
	margin-left: 0
}
#features_section .product-tooltips .tooltips-wrapper .popover .popover-content {
	padding: 0
}
#features_section .product-tooltips .tooltips-wrapper .tooltip-node {
	position: absolute;	width: 38px;	height: 38px;	margin: -20px 0 0 -20px;	opacity: 1;	cursor: pointer;	background-image: url('{{ 'tooltip-node.png' | asset_url}}');	-webkit-transition: all 1s ease;	-moz-transition: all 1s ease;	-ms-transition: all 1s ease;	-o-transition: all 1s ease;	transition: all 1s ease
}
#features_section .product-tooltips .tooltips-wrapper .tooltip-node:hover,  #features_section .product-tooltips .tooltips-wrapper .tooltip-node.active {
	background-image: url('{{ 'tooltip-node-active.png' | asset_url}}')
}
#features_section .product-tooltips .tooltips-wrapper .tooltip_content {
	display: none;	width: 450px;	z-index: 98;	font-style: normal;	font-weight: 300
}
