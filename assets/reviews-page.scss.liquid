.youtube-reviews{
    margin-top: 48px;
    @media screen and (max-width: 430px) {
        margin-top: 32px;
    }
    .youtube-reviews_container {
        display: flex;
        flex-direction: column;
        align-items: center;
        gap: 48px;
        width: 100%;
        max-width: calc(100vw - 180px);
        @media screen and (max-width: 430px) {
            max-width: 382px;
            gap: 24px;
        }
        .youtube-reviews_title{
            font-family: 'Oxanium';
            font-style: normal;
            font-weight: 400;
            font-size: 24px;
            line-height: 30px;
            text-align: center;
            text-transform: uppercase;
            color: #000000;
            @media screen and (max-width: 430px) {
                font-size: 20px;
                line-height: 25px;
            }
            .youtube-reviews__title{
                display: flex;
                align-items: center;
            }
        }
        .youtube-reviews_content{
            width: 100%;
            display: flex;
            flex-direction: row;
            align-items: flex-start;
            padding: 0px;
            gap: 24px;
            .video-youtube-item{
                margin: 0 12px;
                @media screen and (max-width: 430px) {
                    width: 100%;
                }
                a{
                    position: relative;
                    @media screen and (max-width: 430px) {
                        width: 100%;
                    }
                    .play-icon{
                        position: absolute;
                        top: calc(50% - 21px);
                        left: calc(50% - 16px);
                        width: 32px;
                        height: 42px;
                    }
                }
            }
        }
    }
    &.title-left{
        .youtube-reviews_container{
            gap: 24px;
            .youtube-reviews_title{
                display: flex;
                width: 100%;
                justify-content: space-between;
                @media screen and (max-width: 430px) {
                    justify-content: center;
                    font-size: 16px;
                    line-height: 20px;
                    .reviews-pagination{
                        display: none;
                    }
                }
            }
            & > .reviews-pagination{
                display: none;
                @media screen and (max-width: 430px) {
                    display: block;
                }
            }
        }
    }
}



