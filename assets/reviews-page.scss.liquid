#slider-pagination-list {
    display: flex;
    align-items: flex-start;
    padding: 0;
    height: 32px;
    flex-direction: row;
    flex-wrap: nowrap;
    margin: 0;
    list-style: none;
    li {
        margin-bottom: 0;
        a{
            width: 32px;
            height: 32px;
            font-family: Oxanium;
            font-style: normal;
            font-weight: 400;
            font-size: 14px;
            line-height: 18px;
            display: flex;
            align-items: center;
            justify-content: center;
            text-align: center;
            letter-spacing: .16px;
            color: #161616;
            position: relative;
            &.text-white{
                color: #fff;
            }
            &.active{
                &.text-white{
                    &:before{
                        background: #fff;
                    }
                }
                &:before{
                    content: "-";
                    display: block;
                    position: absolute;
                    bottom: 0;
                    width: 16px;
                    height: 2px;
                    background: #000000;
                    color: transparent;
                }
            }
        }
    }
}




