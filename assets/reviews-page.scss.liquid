            }
            .other-review-item {
                display: flex;
                flex-direction: row;
                align-items: flex-start;
                padding: 0;
                gap: 20px;
                position: relative;
                .image-review{
                    width: 55%;
                    min-width: 55%;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    height: 100%;
                    position: relative;
                    border: 1px solid #7F7F7F;
                    border-radius: 4px;
                    padding: 30px 27px;
                    a{
                        position: absolute;
                        top: 12px;
                        right: 12px;
                    }
                    img{
                        width: auto;
                    }
                }
                .description-review{
                    overflow: hidden;
                    max-height: 100%;
                    font-style: normal;
                    font-weight: 400;
                    font-size: 16px;
                    line-height: 20px;
                    color: #000000;
                    position: absolute;
                    right: 0;
                    height: 100%;
                    width: 40%;
                    @media screen and (max-width: 430px) {
                        font-size: 14px;
                        line-height: 18px;
                    }
                    p{
                        margin-bottom: 0;
                    }
                    .overlay{
                        width: 100%;
                        height: 70px;
                        position: absolute;
                        bottom: 0;
                        background: linear-gradient(180deg, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0.5) 19.79%, #FFFFFF 100%);
                    }
                }
            }
        }
        & + .slider-pagination-custom{
            display: none;
        }
    }
    & + .other-content_title{
        display: none !important;
        @media screen and (max-width: 430px) {
            display: flex !important;
            .other-reviews__title{
                display: none;
            }
            .reviews-pagination{
                display: block !important;
            }
        }
    }
}

