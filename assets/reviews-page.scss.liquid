.page-reviews {
    display: flex;
    flex-direction: column;
    align-items: center;
    @media screen and (max-width: 430px) {
        /* gap: 32px; */
    }
}

.page-title {
    font-family: 'Oxanium';
    font-style: normal;
    font-weight: 400;
    font-size: 40px;
    line-height: 50px;
    text-align: center;
    color: #000000;
    text-transform: uppercase;
    height: 80px;
    @media screen and (max-width: 430px) {
        font-size: 32px;
        line-height: 40px;
        height: 40px;
    }
}

.press-reviews {
    margin-top: 80px;
    @media screen and (max-width: 430px) {
        margin-top: 56px;
    }
    .product-review-tabs{
        box-sizing: border-box;
        align-items: flex-start;
        padding: 0px;
        border: 1px solid #7F7F7F;
        border-radius: 4px;
        flex-direction: row;
        display: flex;
        .product-reviews-tab {
            height: 52px;
            box-sizing: border-box;
            display: flex;
            flex-direction: row-reverse;
            align-items: center;
            padding: 6px 16px 6px 8px;
            gap: 2px;
            background: #ffffff;
            border: 2px solid #7f7f7f;
            width: max-content;
            cursor: pointer;
            @media screen and (max-width: 430px) {
                border: 1px solid #7f7f7f;
                padding: 5px 15px 5px 7px;
            }
            &.active{
                background: #F2F2F2;
                .product_title {
                    display: flex;
                }
            }
            .product_title {
                font-family: 'Oxanium';
                font-style: normal;
                font-weight: 400;
                font-size: 20px;
                line-height: 25px;
                text-align: center;
                text-transform: uppercase;
                color: #000000;
                @media screen and (max-width: 430px) {
                    font-size: 14px;
                    line-height: 18px;
                    display: none;
                }
            }
            .product_image{
                width: 40px;
                height: 40px;
                img{
                    width: 100%;
                    height: 100%;
                }
            }
            .product_text {
                font-family: 'Oxanium';
                font-style: normal;
                font-weight: 400;
                font-size: 14px;
                line-height: 18px;
                text-align: center;
                color: #000000;
            }
        }
    }
    .press-reviews_container {
        display: flex;
        flex-direction: column;
        align-items: center;
        gap: 48px;
        width: 100%;
        max-width: calc(100vw - 180px);
        @media screen and (max-width: 430px) {
            max-width: 382px;
            gap: 32px;
        }
        .from-press_desktop {
            display: flex;
            flex-direction: column;
            align-items: center;
            padding: 0px;
            gap: 24px;
            max-width: 100%;
            overflow: hidden;
            .press-review_title{
                font-family: 'Oxanium';
                font-style: normal;
                font-weight: 400;
                font-size: 24px;
                line-height: 30px;
                text-align: center;
                text-transform: uppercase;
                color: #000000;
                @media screen and (max-width: 430px) {
                    font-size: 20px;
                    line-height: 25px;
                }
            }
            .press-reviews_tab{
                width: 100%;
                display: none;
                &.active{
                    display: block;
                }
                .press-reviews_content{
                    width: 100%;
                    .press-reviews-item {
                        display: flex;
                        flex-direction: row;
                        align-items: center;
                        padding: 0;
                        gap: 56px;
                        justify-content: center;
                        flex-wrap: wrap;
                        @media screen and (min-width: 431px) and (max-width: 1439px) {
                            flex-direction: column;
                        }
                        @media screen and (max-width: 430px) {
                            flex-direction: column;
                            gap: 24px;
                        }
                        .press-blog_one-row {
                            padding: 24px;
                            box-sizing: border-box;
                            border: 1px solid #7F7F7F;
                            border-radius: 4px;
                            display: flex;
                            flex-direction: column;
                            position: relative;
                            width: 33.333333%;
                            @media screen and (min-width: 431px) and (max-width: 1439px) {
                                width: 100%;
                            }
                            @media screen and (max-width: 430px) {
                                width: 100%;
                            }
                            .image-blog_1{
                                padding: 27.67px calc(50% - 257.78px/2 - 0.11px);
                                @media screen and (max-width: 430px) {
                                    padding: 18px 30px;
                                }
                            }
                            .description{
                                position: relative;
                                font-size: 16px;
                                line-height: 20px;
                                overflow: hidden;
                                max-height: 81px;
                                @media screen and (max-width: 430px) {
                                    max-height: 65px;
                                }
                                p{
                                    margin-bottom: 0;
                                }
                                .overlay{
                                    z-index: 9999;
                                    position: absolute;
                                    height: 45px;
                                    width: 100%;
                                    bottom: 0;
                                    background: linear-gradient(180deg, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0.5) 28.65%, #FFFFFF 100%);
                                }
                            }
                            a{
                                position: absolute;
                                top: 18.5px;
                                right: 18.5px;
                            }
                        }
                        .press-blog_two-row {
                            display: flex;
                            flex-direction: column;
                            align-items: flex-start;
                            padding: 0px;
                            gap: 24px;
                            width: 28.88888%;
                            @media screen and (min-width: 431px) and (max-width: 1439px) {
                                width: 100%;
                                flex-direction: row;
                            }
                            @media screen and (max-width: 430px) {
                                width: 100%;
                            }
                            .press-blog_2,
                            .press-blog_3,
                            .press-blog_4,
                            .press-blog_5 {
                                display: flex;
                                flex-direction: row;
                                align-items: flex-start;
                                padding: 0px;
                                gap: 20px;
                                position: relative;
                                @media screen and (max-width: 430px) {
                                    gap: 16px;
                                }
                                @media screen and (min-width: 431px) and (max-width: 1439px) {
                                    width: 50%;
                                    flex-direction: row;
                                }
                                .image-blog {
                                    box-sizing: border-box;
                                    border: 1px solid #7F7F7F;
                                    border-radius: 4px;
                                    padding: 30px 27px;
                                    display: flex;
                                    min-width: 55%;
                                    width: 55%;
                                    position: relative;
                                    a{
                                        position: absolute;
                                        width: 12px;
                                        height: 12px;
                                        top: 12px;
                                        right: 12px;
                                        display: flex;
                                    }
                                }
                                .description {
                                    height: 140px;
                                    font-family: Oxanium;
                                    font-style: normal;
                                    font-weight: 400;
                                    font-size: 16px;
                                    line-height: 20px;
                                    color: #000;
                                    overflow: hidden;
                                    position: relative;
                                    @media screen and (min-width: 431px) and (max-width: 1439px) {
                                        position: absolute;
                                        right: 0;
                                        height: 100%;
                                        width: 40%;
                                    }
                                    .overlay{
                                        position: absolute;
                                        height: 70px;
                                        width: 100%;
                                        background: linear-gradient(180deg, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0.5) 19.79%, #FFFFFF 100%);
                                        z-index: 9999999;
                                        bottom: 0;
                                    }
                                }
                            }
                        }
                    }
                }
                .reviews-pagination{
                    margin-top: 24px;
                }
            }
        }
    }
}
