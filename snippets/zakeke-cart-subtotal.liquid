{% assign total_price = cart.total_price %}
{% for item in cart.items %}
	{% assign customization_price = item.properties._configurationPrice | default: 0 | times: item.quantity %}
    {% if customization_price > 0 %}
		{% assign total_price = total_price | plus: customization_price %}
	{% endif %}
{% endfor %}
{{ total_price | money }}