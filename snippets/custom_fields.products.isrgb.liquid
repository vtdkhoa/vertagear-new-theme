{% comment %}
  DO NOT MODIFY THIS FILE DIRECTLY. THIS FILE WILL BE OVERWRITTEN.
  YOU CAN MODIFY THE LIQUID CODE FOR THIS CUSTOM FIELD HERE:
  https://customfields.bonify.io/dashboard/vertagear.myshopify.com/products/configure/71341/edit
{% endcomment %}
{% if product.metafields.custom_fields["isrgb"] != blank %}
  <div class="custom-field custom-field__isrgb custom-field__type--checkbox">
    <h3 class="custom-field--title">is-rgb</h3>
    <div class="custom-field--value">
      {% if product.metafields.custom_fields["isrgb"] == 1 %}
        TRUE
      {% else %}
        FALSE
      {% endif %}
    </div>
  </div>
{% endif %}